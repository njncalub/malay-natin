{
  "name": "malay-natin",
  "version": "0.0.1",
  "description": "Interactive itinerary for my upcoming trip to Malaysia.",
  "main": "index.js",
  "dependencies": {
    "core-js": "^3.2.1",
    "leaflet": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.3",
    "parcel-bundler": "^1.12.3",
    "push-dir": "^0.4.1",
    "sass": "^1.22.10",
    "shx": "^0.3.2",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "start": "parcel ./index.html",
